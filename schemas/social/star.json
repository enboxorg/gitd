{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://enbox.org/schemas/forge/star",
  "title": "StarData",
  "description": "A star marking a repository as a favorite",
  "type": "object",
  "properties": {
    "repoDid": {
      "type": "string"
    },
    "repoRecordId": {
      "type": "string"
    },
    "repoName": {
      "type": "string"
    }
  },
  "required": ["repoDid", "repoRecordId"],
  "additionalProperties": false
}
