{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://enbox.org/schemas/forge/issue",
  "title": "IssueData",
  "description": "An issue in a forge repository",
  "type": "object",
  "properties": {
    "title": {
      "type": "string"
    },
    "body": {
      "type": "string"
    },
    "number": {
      "type": "number"
    }
  },
  "required": ["title", "body"],
  "additionalProperties": false
}
