{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://enbox.org/schemas/forge/org",
  "title": "OrgData",
  "description": "An organization that owns repositories and has members",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "displayName": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "avatar": {
      "type": "string"
    },
    "website": {
      "type": "string"
    }
  },
  "required": ["name"],
  "additionalProperties": false
}
