{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://enbox.org/schemas/forge/patch",
  "title": "PatchData",
  "description": "A forge patch proposal containing a title, body, and optional sequence number.",
  "type": "object",
  "properties": {
    "title": {
      "type": "string"
    },
    "body": {
      "type": "string"
    },
    "number": {
      "type": "number"
    }
  },
  "required": ["title", "body"],
  "additionalProperties": false
}
